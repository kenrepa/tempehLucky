    <section id="pricing-table">
        <div class="container">
            <div class="row">
                <div class="title">
                    <h2>@Html.Label("", Gweb.Resources.Language.pricingtitle)</h2>
                    <p>@Html.Label("", Gweb.Resources.Language.pricingtxt1)<br>@Html.Label("", Gweb.Resources.Language.pricingtxt2)</p>
                </div>
                <div class="col-md-4 col">
                    <div class="block text-center wow fadeInLeft" data-wow-delay=".3s">
                        <ul>
                            <li>
                                <h4>@Html.Label("", Gweb.Resources.Language.regprice)</h4>
                                <p>&#36; 6 <span>/pcs</span></p>
                            </li>
                            <li>
                                <img src="~/img/tempeh_img1.png" class="img-thumbnail" alt="tempeh1">
                            </li>
                            <li>
                                @using (Html.BeginForm("PayPaymentWithPaypal", "Home", FormMethod.Post))
                                {
                                    <div style="display:inline-block">
                                        <span style="float:left"><b>@Html.Label("", Gweb.Resources.Language.quantity)&nbsp;&nbsp; </b></span>
                                        <span style="float:right">@Html.TextBoxFor(m => m.Quantity, new { @class = "form-control", style = "width:70px;", maxLength = "2" })</span>
                                    </div>
                                    @Html.HiddenFor(m => m.Price, new { Value = "6" })
                                    @Html.HiddenFor(m => m.ItemName, new { Value = "Regular Tempeh 300g" })
                                    <br />
                                    <input type="submit" class="btn btn-buy" value="Buy" />
                                }
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-4 col">
                    <div class="block text-center wow zoomIn" data-wow-delay=".3s">
                        <ul>
                            <li>
                                <h4>@Html.Label(Gweb.Resources.Language.gmoprice)</h4>
                                <p>&#36; 10 <span>/pcs</span></p>
                            </li>
                            <li>
                                <img src="~/img/tempeh_img2.png" class="img-thumbnail" alt="tempeh1">
                            </li>
                            <li>
                                @using (Html.BeginForm("PayPaymentWithPaypal", "Home", FormMethod.Post))
                                {
                                    <div style="display:inline-block">
                                        <span style="float:left"><b>@Html.Label("", Gweb.Resources.Language.quantity)&nbsp;&nbsp; </b></span>
                                        <span style="float:right">@Html.TextBoxFor(m => m.Quantity, new { @class = "form-control", style = "width:70px;", maxLength = "2" })</span>
                                    </div>
                                    @Html.HiddenFor(m => m.Price, new { Value = "10" })
                                    @Html.HiddenFor(m => m.ItemName, new { Value = "Non-GMO Tempeh 300g" })
                                    <br />
                                    <input type="submit" class="btn btn-buy" value="Buy" />

                                }
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-4 col">
                    <div class="block text-center wow fadeInRight" data-wow-delay=".3s">
                        <ul>
                            <li>
                                <h4>@Html.Label("", Gweb.Resources.Language.barprice)</h4>
                                <p>&#36; 6 <span>/pcs</span></p>
                            </li>
                            <li>
                                <img src="~/img/tempeh2.jpg" class="img-thumbnail" alt="tempeh1">
                            </li>
                            <li>
                                <!-- Replace "test" with your own sandbox Business account app client ID -->
                                @using (Html.BeginForm("PayPaymentWithPaypal", "Home", FormMethod.Post))
                                {
                                    <div style="display:inline-block">
                                        <span style="float:left"><b>@Html.Label("", Gweb.Resources.Language.quantity)&nbsp;&nbsp; </b></span>
                                        <span style="float:right">
                                            @Html.TextBoxFor(m => m.Quantity, new { @class = "form-control", style = "width:70px;", maxLength = "2" })
                                            @Html.ValidationMessageFor(m => m.Quantity, "", new { @class = "error" })
                                        </span>
                                    </div>
                                    @Html.HiddenFor(m => m.Price, new { Value = "6" })
                                    @Html.HiddenFor(m => m.ItemName, new { Value = "Tempeh Bar 150g" })
                                    <br />
                                    <input type="submit" class="btn btn-buy" value="Buy" />
                                }
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>

